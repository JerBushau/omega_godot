code = "shader_type canvas_item;

uniform vec3 color = vec3(0.41, 0.41, 0.41);
uniform int OCTAVES = 3;

float rand(vec2 coord){
	return fract(sin(dot(coord, vec2(55, 55)) * 1000.0) * 1000.0);
}

float noise(vec2 coord){
	vec2 i = floor(coord);
	vec2 f = fract(coord);

	// 4 corners of a rectangle surrounding our point
	float a = rand(i);
	float b = rand(i + vec2(1.0, 0.0));
	float c = rand(i + vec2(0.0, 1.0));
	float d = rand(i + vec2(1.0, 1.0));

	vec2 cubic = f * f * (3.0 - 2.0 * f);

	return mix(a, b, cubic.x) + (c - a) * cubic.y * (1.0 - cubic.x) + (d - b) * cubic.x * cubic.y;
}

float fbm(vec2 coord){
	float value = 0.0;
	float scale = 0.5;

	for(int i = 0; i < OCTAVES; i++){
		value += noise(coord) * scale;
		coord *= 4.0;
		scale *= 2.5;
	}
	return value;
}

void fragment() {
//	COLOR = texture(TEXTURE, UV * 1.0);
	
	vec2 coord = UV * 20.0;
	vec2 motion = vec2( fbm(coord + vec2(TIME * -0.7, TIME * 0.7)) );

	float final = fbm(coord + motion);
	
	COLOR.rgb = color;
	COLOR.a = final * 0.039;
//    COLOR.a = 1.0 - texture(TEXTURE, (UV - 0.5) * vec2(1, 1) + 0.5).a;
}"

